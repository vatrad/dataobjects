{
    "name": "vatradar/dataobjects",
    "description": "VATSIM Data Objects",
    "type": "library",
    "license": "LGPL-3.0",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "VATRadar Devs",
            "email": "dev@vatradar.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "VatRadar\\DataObjects\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\DataObjects\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "psy/psysh": "^0",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpunit/phpunit": "^10.0",
        "phpstan/extension-installer": "^1.2",
        "spaze/phpstan-disallowed-calls": "^2.11",
        "phpstan/phpstan-phpunit": "^1.3"
    },
    "config": {
        "bin-dir": "vbin",
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.6-dev"
        }
    }
}
